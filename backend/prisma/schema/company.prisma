model Company{
  id        String     @id @default(uuid())
  createdAt DateTime   @default(now())
  updatedAt DateTime   @default(now())
  deletedAt DateTime?
  logo      String

  leaders Staff[] @relation("CompanyLeaders")
  staffs Staff[]

  addressId String
  address Address @relation(fields: [addressId], references: [id])
  name      String     @unique
  email     String
  phone1    String
  phone2    String?
}